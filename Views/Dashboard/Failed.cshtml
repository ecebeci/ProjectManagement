@{
    if (ViewData["Title"] == null) { ViewData["Title"] = "Failed"; }

   
}

<div class="card border-danger">
    <div class="card-body">
        <h5 class="card-title text-danger">@ViewBag.Title</h5>
        <p class="card-text lead">@ViewBag.Message</p>
        <a class="btn btn-primary" asp-action="Index" asp-route-id="@ViewBag.BoardId">View Lists</a>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}